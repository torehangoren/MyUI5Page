<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:forms="sap.ui.layout.form" xmlns="sap.m" 
	xmlns:core="sap.ui.core"
	xmlns:f="sap.f">

	<List
		items="{/courses}">
		<StandardListItem
			title="{courseNo}"
			description="{courseDetail}"
			iconDensityAware="false"
			iconInset="false" 
			/>
	</List>

</mvc:View>
